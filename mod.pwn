		return 341;

	case 10..15:
		return weaponid+311;

	case 16..18:
		return weaponid+326;

	case 22..29:
		return weaponid+324;

	case 30,31:
		return weaponid+325;

	case 32:
		return 372;

	case 33..45:
		return weaponid+324;

