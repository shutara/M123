	foreach (Player, i)
	{
		DeletePlayer3DTextLabel(i,Gryz3D[i]);
		if(IsPlayerConnected(i))
		{
			if(ProrabID[i] != 999)
			{
				GetPlayerName(ProrabID[i], name, sizeof(name));
				format(string, sizeof(string), "[Информация]\nВаш прораб\n%s",name);
				Gryz3D[i] = CreatePlayer3DTextLabel( i, string, 0x008080FF, 2175.6513671875,-2259.1384277344,15.573437690735, 30.0);
				//Pro[playerid2] = CreatePlayer3DTextLabel( playerid2, string, 0x008080FF, 2175.6513671875,-2259.1384277344,15.573437690735, 40.0);
			}
			if(PlayerInJob2[i])
			{
				for(new j=0; j<MAX_PLAYER; j++)
				{
					if(IsPlayerConnected(j))
					{
						if(ProrabID[j] == i)
						{
							GetPlayerName(j, name, sizeof(name));
							format(string2, sizeof(string2), "%s\n%s (%d мешков)",string2,name,Meshki[j]);
