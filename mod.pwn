public OnPlayerSelectedMenuRow(playerid, row)
{
	new Float:health;
	new Menu:current;
	new sendername[256];
	new string[256];
	new text[256];
	GetPlayerHealth(playerid,health);
	current = GetPlayerMenu(playerid);
	new Menu:CurrentMenu = GetPlayerMenu(playerid);
	if(CurrentMenu == burgermenu || CurrentMenu == chickenmenu || CurrentMenu == pizzamenu)
	{
		TogglePlayerControllable(playerid, 1);
		if (row != 10) // 10 = Leave
		{
			if (PlayerInfo[playerid][pCash] <= 0)
			{
				GameTextForPlayer(playerid, FixText("~r~нехватает денег"), 5000, 1);
				return 1;
			}
			new actstring[128];
			GetPlayerName(playerid, sendername, sizeof(sendername));
			GetPlayerHealth(playerid, health);
			if (row == 7 || row == 8) // Drink
			{
				format(actstring, sizeof(actstring), "* %s пьет из чашки.", sendername);
				ServerGivePlayerMoney(playerid, -50);
				SetPlayerHealth(playerid, health + 10);
				ApplyAnimation(playerid,"VENDING","VEND_Drink_P",4.1,0,1,1,0,1);
			}
			else // Meal
			{
				switch (row)
				{
				case 1:
					{
						format(actstring, sizeof(actstring), "* %s ест свой детский ужин и играет с маленькой игрушкой, которая к нему прилагается.", sendername);
						ServerGivePlayerMoney(playerid, -50);
						SetPlayerHealth(playerid, health + 20);
					}
				case 2:
					{
						if (CurrentMenu == chickenmenu) format(actstring, sizeof(actstring), "* %s ест наггетсы из своей чашки.", sendername);
						else if (CurrentMenu == pizzamenu) format(actstring, sizeof(actstring), "* %s ест свою пиццу.", sendername);
						else format(actstring, sizeof(actstring), "* %s ест свой чизбургер.", sendername);
						ServerGivePlayerMoney(playerid, -150);
						SetPlayerHealth(playerid, health + 50);
					}
				case 3:
					{
						if (CurrentMenu != pizzamenu) format(actstring, sizeof(actstring), "* %s ест свой жирный сэндвич.", sendername);
						else format(actstring, sizeof(actstring), "* %s ест жирный кусок пиццы.", sendername);
						ServerGivePlayerMoney(playerid, -300);
						SetPlayerHealth(playerid, health + 80);
					}
				case 4:
					{
						format(actstring, sizeof(actstring), "* %s ест свеженарезанный салат из своей миски.", sendername);
						ServerGivePlayerMoney(playerid, -100);
						SetPlayerHealth(playerid, health + 35);
					}
				}
				if (CurrentMenu == burgermenu) ApplyAnimation(playerid,"FOOD","EAT_Burger",4.1,0,1,1,0,1);
				else if (CurrentMenu == chickenmenu) ApplyAnimation(playerid,"FOOD","EAT_Chicken",4.1,0,1,1,0,1);
				else if (CurrentMenu == pizzamenu) ApplyAnimation(playerid,"FOOD","EAT_Pizza",4.1,0,1,1,0,1);
				if (PlayerInfo[playerid][pCash] > 100) SetPlayerHealth(playerid, 100);
			}
			ProxDetector(30.0, playerid, actstring, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		}
	}
	if(current == bomjmenu)
	{
		switch(row)
		{
		case 0:
			{
				SendClientMessage(playerid, COLOR_GREEN, "case0");
			}
		case 1:
			{
				if(PlayerInfo[playerid][pSex] == 1)
				{
					if(SelectCharPlace[playerid] == 1) { SetPlayerSkin(playerid, JoinPed[90][0]); SelectCharPlace[playerid] = 2; ChosenSkin[playerid] = JoinPed[90][0]; }
					else if(SelectCharPlace[playerid] == 2) { SetPlayerSkin(playerid, JoinPed[91][0]); SelectCharPlace[playerid] = 3; ChosenSkin[playerid] = JoinPed[91][0]; }
					else if(SelectCharPlace[playerid] == 3) { SetPlayerSkin(playerid, JoinPed[93][0]); SelectCharPlace[playerid] = 4; ChosenSkin[playerid] = JoinPed[93][0]; }
					else if(SelectCharPlace[playerid] == 4) { SetPlayerSkin(playerid, JoinPed[94][0]); SelectCharPlace[playerid] = 5; ChosenSkin[playerid] = JoinPed[94][0]; }
					else if(SelectCharPlace[playerid] == 5) { SetPlayerSkin(playerid, JoinPed[95][0]); SelectCharPlace[playerid] = 6; ChosenSkin[playerid] = JoinPed[95][0]; }
					else if(SelectCharPlace[playerid] == 6) { SetPlayerSkin(playerid, Peds[18][0]); SelectCharPlace[playerid] = 7; ChosenSkin[playerid] = Peds[18][0]; }
					else if(SelectCharPlace[playerid] == 7) { SetPlayerSkin(playerid, JoinPed[92][0]); SelectCharPlace[playerid] = 1; ChosenSkin[playerid] = JoinPed[92][0]; }
					ShowMenuForPlayer(bomjmenu,playerid);
				}
