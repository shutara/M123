public OnPlayerExitedMenu(playerid)//Выполняется, когда игрок выходит из меню без выбора пункта, кнопкой "Enter".
{
	new Menu:Current = GetPlayerMenu(playerid);//получить ID меню
	if(Current == mAmmuNation)//для амуниций
	{
		HideMenuForPlayer(mAmmuNation,playerid);
		TogglePlayerControllable(playerid, true);//освободить
	}
	if(Current == McDriveMenu)
	{
		HideMenuForPlayer(McDriveMenu,playerid);
		TogglePlayerControllable(playerid, true);//освободить
	}
	return 1;
}

public OnVehicleDamageStatusUpdate(vehicleid, playerid)
{
	new Float:health;
	GetPlayerHealth(playerid, health);
	if(PlayerInfo[playerid][pSeatBelt] == 1)
	{
		SetPlayerHealth(playerid, health -0);
	}
	else SetPlayerHealth(playerid, health -4);
	return true;
}

