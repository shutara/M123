					}
					else
					{
						SendClientMessage(playerid, COLOR_WHITE, "* Invalid transaction amount.");
					}
				}
			}
			else
			{
				format(string, sizeof(string), "* %d is not an active player.", giveplayerid);
				SendClientMessage(playerid, COLOR_WHITE, string);
			}
		}
		return 1;
	}
	return 1;
}
