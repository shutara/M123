		GetPlayerName(playerid, sendername, sizeof(sendername));
  if (PlayerInfo[playerid][pSex] == 1)
  {
		format(string, sizeof(string), "%s сказал : %s", GetPlayerNameEx(playerid), text);
  }
  else
  {
        format(string, sizeof(string), "%s сказала : %s", GetPlayerNameEx(playerid), text);
  }
		ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
		SetPlayerChatBubble(playerid, text, 0xC8FFFFFF, 30.0, 10000);
		//ApplyAnimation(playerid,"ped","IDLE_chat",4,0,0,0,0,1500);
		return 0;
	}
	return 1;
}

public FixHour(hour)
{
	hour = timeshift+hour+1;
	if (hour < 0)
	{
		hour = hour+24;
	}
	else if (hour > 23)
	{
		hour = hour-24;
	}
	shifthour = hour;
	return 1;
}

