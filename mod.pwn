public StatusMotor(playerid)
{
	new c = GetPlayerVehicleID(playerid);
	new sendername[MAX_PLAYER_NAME];
	new string[64];
	if(Battery[c]<3)
	{
		Engine(c,false);
		GetVehicleParamsEx(c,engine,lights,alarm,doors,bonnet,boot,objective);
		SetVehicleParamsEx(c,VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
		format(string, sizeof(string), "* %s Не заводится", sendername);
		ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	}
	if(Battery[c]>3)
	{
		GetPlayerName(playerid, sendername, sizeof(sendername));
		GetVehicleParamsEx(c,engine,lights,alarm,doors,bonnet,boot,objective);
		SetVehicleParamsEx(c,VEHICLE_PARAMS_ON,lights,alarm,doors,bonnet,boot,objective);
		format(string, sizeof(string), "* %s заводит двигатель", sendername);
		ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		SetPVarInt(playerid, "StatusMotor", 0);
		Engine(c,true);
		Battery[c] -= 1;
	}
	if(CarInfo[c][cNeon] != 0)
