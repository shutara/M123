public Lotto(number)
{
	new JackpotFallen = 0;
	new string[256];
	new winner[MAX_PLAYER_NAME];
	format(string, sizeof(string), "* Новости лотереи: Сегодня выигрышный номер: %d.", number);
	OOCOff(COLOR_WHITE, string);
	foreach (Player, i)
	{
		if(IsPlayerConnected(i))
		{
			if(PlayerInfo[i][pLottoNr] > 0)
			{
				if(PlayerInfo[i][pLottoNr] == number)
				{
					JackpotFallen = 1;
					GetPlayerName(i, winner, sizeof(winner));
					format(string, sizeof(string), "* Новости лотереи: %s выиграл джекпот $%d.", winner, Jackpot);
					OOCOff(COLOR_WHITE, string);
					format(string, sizeof(string), "* Вы выиграли $%d в лотерее! Поздравляем!!!", Jackpot);
					SendClientMessage(i, COLOR_YELLOW, string);
					ServerGivePlayerMoney(i, Jackpot);
				}
				else
				{
					SendClientMessage(i, COLOR_LIGHTBLUE, "* Вы не выиграли в лотерею на этот раз.");
				}
			}
			PlayerInfo[i][pLottoNr] = 0;
		}
	}
	if(JackpotFallen)
	{
		new rand = random(125000); rand += 15789;
		Jackpot = rand;
		SaveStuff();
		format(string, sizeof(string), "* Новости лотереи: Новый Джекпот начинается с $%d.", Jackpot);
		OOCOff(COLOR_WHITE, string);
	}
	else
	{
		new rand = random(15000); rand += 2158;
		Jackpot += rand;
		SaveStuff();
		format(string, sizeof(string), "* Новости лотереи: Сумма Джекпота возросла до $%d.", Jackpot);
		OOCOff(COLOR_WHITE, string);
	}
	return 1;
}
