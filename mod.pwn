public Question()
{
	if(strlen(answer))
	{
		format(answer, 256, "[Dj Денис Воронцов]: Жаль, но никто не смог правильно ответить на наш вопрос!", answer);
		SendClientMessageToAll(COLOR_NEWS, answer);
		KillTimer(QuestionTimer);
		QuestionTimer=SetTimer("Question", 270000, 1);
		format(answer, 256, "");
		return 1;
	}
	if(random(2)==1)
	{
		new d=random(sizeof(questions));
		new string[256];
		new podsk[256];
		format(string, 256, "%s",questions[d]);
		strmid(question, string, 0, strfind(string, "^", true), 256);
		strmid(answer, string, strfind(string, "^", true)+1, strlen(string), 256);
		new len=strlen(answer);
		format(podsk, 256, "%d", strlen(answer));
		strmid(podsk, podsk, strlen(podsk)-1, strlen(podsk), 256);
		new a=strval(podsk);
		if(a==1&&len!=11) format(string, 256, "%d буква", len);
		if(a>=2&&a<=4&&len!=12&&len!=13&&len!=14) format(string, 256, "%d буквы", len);
		if(a==0||(a>=5&&a<=9)||(len>=11&&len<=14)) format(string, 256, "%d букв", len);
		format(string, 256, "[Dj Денис Воронцов]: %s (%s)", question, string);
		SendClientMessageToAll(COLOR_NEWS, string);
		format(string, 256, "[Dj Денис Воронцов]: Отправляйте свой ответ на номер {92000a}7575!", string);
		SendClientMessageToAll(COLOR_NEWS, string);
	}
	else
	{
		new a=random(100), b=random(100), c=random(4), d=random(10), f=random(100);
		switch(c)
		{
		case 0:
			{
				format
				(
				question, 256, "[Dj Денис Воронцов]: %d+%d-%d=? (( /sms 7575 ответ ))",a,b,d); format(answer, 256, "%d", a+b-d);
			}
		case 1:
			{
				format
				(
				question, 256, "[Dj Денис Воронцов]: %d-%d+%d=? (( /sms 7575 ответ ))",a,b,d); format(answer, 256, "%d", a-b+d);
			}
		case 2:
			{
				format
				(
				question, 256, "[Dj Денис Воронцов]: %d*%d-%d=? (( /sms 7575 ответ ))",a,b,f); format(answer, 256, "%d", a*b-f);
			}
		case 3:
			{
				format
				(
				question, 256, "[Dj Денис Воронцов]: %d*%d+%d=? (( /sms 7575 ответ ))",a,b,f); format(answer, 256, "%d", a*b+f);
			}
		}
		SendClientMessageToAll(COLOR_NEWS, question);
	}
	return 0;
}

