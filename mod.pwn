public SetPlayerUnjail()
{
	new plname[MAX_PLAYER_NAME];
	new sendername[MAX_PLAYER_NAME];
	new string[256];
	if(PaintballPlayers >= 2 && PaintballRound != 1 && StartingPaintballRound != 1)
	{
		StartingPaintballRound = 1;
		SetTimer("PreparePaintball", 15000, 0);
	}
	if(KartingPlayers >= 2 && KartingRound != 1 && StartingKartRound != 1)
	{
		StartingKartRound = 1;
		SetTimer("PrepareKarting", 15000, 0);
	}
	if(KartingRound != 0 && KartingPlayers < 2)
	{
		StartingKartRound = 0;
		KartingRound = 0;
		EndingKartRound = 1;
	}
	if(ReportLimit > 0) ReportLimit--;
	if(VoprosLimit > 0) VoprosLimit--;
	if(adlimit > 0) adlimit--;
	if(viplimit > 0) viplimit--;
	if(war > 0) war --;
	foreach (Player, i)
	{
		//==============================================================================
		new level = PlayerInfo[i][pLevel];
		if(level >= 0 && level <= 4) { PlayerInfo[i][pPayCheck] += 1; }
		else if(level >= 5 && level <= 6) { PlayerInfo[i][pPayCheck] += 2; }
		else if(level >= 7 && level <= 8) { PlayerInfo[i][pPayCheck] += 3; }
		else if(level >= 9 && level <= 10) { PlayerInfo[i][pPayCheck] += 4; }
		else if(level >= 11 && level <= 12) { PlayerInfo[i][pPayCheck] += 5; }
		else if(level >= 13 && level <= 14) { PlayerInfo[i][pPayCheck] += 6; }
		else if(level >= 15 && level <= 16) { PlayerInfo[i][pPayCheck] += 7; }
		else if(level >= 17 && level <= 18) { PlayerInfo[i][pPayCheck] += 8; }
		else if(level >= 19 && level <= 20) { PlayerInfo[i][pPayCheck] += 9; }
		else if(level >= 21 && level <= 22) { PlayerInfo[i][pPayCheck] += 10; }
		else if(level >= 23) { PlayerInfo[i][pPayCheck] += 11; }
