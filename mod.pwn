						PlayerInfo[playa][pJailed] = 1;
						PlayerInfo[giveplayerid][pPlayerCuffed] = 0;
						PlayerInfo[playa][pArrested] += 1;
						WantedPoints[playa] = 0;
						WantLawyer[playa] = 1;
						TogglePlayerControllable(playa, 1);
						PlayerCuffed[playa] = 0;
						PlayerCuffedTime[playa] = 0;
						SetPlayerWantedLevel(playa,0);
						PlayerInfo[playa][pZakon] -= 2;
						PlayerInfo[playa][pPlayerCuffed] = 0;
