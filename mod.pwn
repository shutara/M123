public BankRob()
{
	foreach (Player, i)
	{
		if(IsPlayerConnected(i))
		{
			if(IsARobber[i] == 1)
			{
				new chance = random(10);
				TogglePlayerControllable(i, true);
				ClearAnimations(i);
				if(chance == 1||chance == 2||chance == 3||chance == 4||chance == 5)
				{
					MoveObject(bankobj, 2146.240234375, 1627.0949707031, 994.29321289063,0.2);
					SendRadioMessage(1, COLOR_LIGHTBLUE, "* (рация): Внимание, всем постам, cовершен взлом сейфовой двери!");
					SetPlayerCriminal(i, 255, "Взлом двери сейфа");
					WantedPoints[i] += 5;
					if(chance == 3 || chance == 6 || chance == 9)
					{
						PlayerInfo[i][pSD]--;
						SendClientMessage(i, COLOR_WHITE, "* Вы сломали отвёртку!");
					}
					SendClientMessage(i, COLOR_WHITE, "* Вы успешно открыли дверь!");
					SetTimer("BankClose", 60000, 0);
					bdo = 1;
					IsARobber[i] = 0;
				}
				else if(chance == 6||chance == 7||chance == 8||chance == 9||chance == 10)
				{
					SendRadioMessage(1, COLOR_LIGHTBLUE, "* (рация): Внимание, всем постам, попытка взлома сейфовой двери!");
					SetPlayerCriminal(i, 255, "Взлом двери сейфа");
					WantedPoints[i] += 5;
					if(chance == 3 || chance == 6 || chance == 9)
					{
						PlayerInfo[i][pSD]--;
						SendClientMessage(i, COLOR_WHITE, "* Вы сломали отвёртку!");
					}
					SendClientMessage(i, COLOR_WHITE, "* Вам не удалось открыть дверь!");
					IsARobber[i] = 0;
				}
			}
		}
	}
	return 1;
}
