					{
						new bonus = PlayerInfo[i][pPayCheck] / 2;
						PlayerInfo[i][pPayCheck] += bonus;
					}
					new checks = PlayerInfo[i][pPayCheck];
					new ebill = (PlayerInfo[i][pBank]/10000)*(PlayerInfo[i][pLevel]);
					new avtobill,coltach;
