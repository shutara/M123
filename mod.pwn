stock MafiaPayDay()
{
	foreach (Player, i)
	{
		new bouse = PlayerInfo[i][pPbiskey];
		if(bouse != 255)
		{
			if(BizzInfo[bouse][bMafia] !=0)
			{
				if(BizzInfo[bouse][bTill] > 5000)
				{
					BizzInfo[bouse][bTill] -= 5000;
					if(BizzInfo[bouse][bMafia] == 6){MafiaBank[0][nYakuza] +=5000;}
					if(BizzInfo[bouse][bMafia] == 13){MafiaBank[0][nRm] +=5000;}
					if(BizzInfo[bouse][bMafia] == 5){MafiaBank[0][nLcn] +=5000;}
				}
			}
			if(bouse >= 100)
			{
				if(SBizzInfo[bouse-100][sbMafia] !=0)
				{
					if(SBizzInfo[bouse-100][sbTill] > 5000)
					{
						SBizzInfo[bouse-100][sbTill] -= 5000;
						if(SBizzInfo[bouse-100][sbMafia] == 6){MafiaBank[0][nYakuza] +=5000;}
						if(SBizzInfo[bouse-100][sbMafia] == 13){MafiaBank[0][nRm] +=5000;}
						if(SBizzInfo[bouse-100][sbMafia] == 5){MafiaBank[0][nLcn] +=5000;}
					}
				}
			}
		}
	}
}
