public OnPlayerDisconnect(playerid, reason)
//------------------------------------------------------------------------------------------------------
{
	PlayerInJob[playerid] = 0;
	PlayerInJob2[playerid] = 0;
	Meshok[playerid] = 0;
	MeshokTime[playerid] = 0;
	DalTime[playerid] = 0;
	WeaponTime[playerid] = 0;
	Meshki[playerid] = 0;
	Oldskin2[playerid] = 0;
	KolxozOdejda[playerid] = 0;
	TypeJob[playerid] = 0;
	ProrabID[playerid] = 999;
	Prorab[playerid] = 999;
	vzlommotor[playerid] = 0;
	shield[playerid] = 0;
	TeleportTime[playerid] = 0;
	GunCheckTime[playerid] = 0;
	weaponid_exit[playerid] = 0;
	ammo_exit[playerid] = 0;
	PlayerEx[playerid][VarEx] = 0;
	if(PlayerInfo[playerid][pAdmin] > 0 || PlayerInfo[playerid][pAdmin] == -1) Delete3DTextLabel(laabel[playerid]);
	if(PlayerCuffed[playerid] == 2) PlayerInfo[playerid][pPlayerCuffed] = 1;
	PlayerInfo[playerid][pWanted] = WantedLevel[playerid];
	OnPlayerSave(playerid);
	MRP_DeleteMoneySysForPlayer(playerid);
	O_DeleteMoneySysForPlayer(playerid);
	PlayerInfo[playerid][pMuted]=0;
	new message[127];
	GetPlayerName(playerid, message, sizeof(message));
	format(message,sizeof(message), " %s", message, playerid);
	switch(reason) {
	case 0: format(message,sizeof(message), "{ffffff}%s покинул(а) сервер {ff0000}(Вылетел)", message);
	case 1: format(message,sizeof(message), "{ffffff}%s покинул(а) сервер {ffa500}(Вышел)", message);
	case 2: format(message,sizeof(message), "{ffffff}%s покинул(а) сервер {ffff00}(Кикнут)", message);
	}
	foreach (Player, i) {
		if(IsPlayerConnected(i) && playerid != i && ShowExit[i]) {
			SendClientMessage(i, COLOR_WHITE , message);
		}
	}
	foreach (Player, i)
	{
		if(IsPlayerConnected(i))
		{
			if(TaxiAccepted[i] < 999)
			{
				if(TaxiAccepted[i] == playerid)
				{
					TaxiAccepted[i] = 999;
					GameTextForPlayer(i, FixText("~w~ваш клиент~n~~r~вышел из игры"), 5000, 1);
					TaxiCallTime[i] = 0;
					DisablePlayerCheckpoint(i);
				}
			}
			else if(BusAccepted[i] < 999)
			{
				if(BusAccepted[i] == playerid)
				{
					BusAccepted[i] = 999;
					GameTextForPlayer(i, FixText("~w~ваш клиент~n~~r~вышел из игры"), 5000, 1);
					BusCallTime[i] = 0;
					DisablePlayerCheckpoint(i);
				}
			}
		}
		if(Mobile[i] == playerid)
		{
			if(PhoneMusic[i] == 1)
			{
				PlayerPlaySound(i,1184,0,0,0);
				PhoneMusic[i] = 0;
			}
		}
	}
	for(new x=0; x<MAX_PLAYER; x++) {
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && gSpectateID[x] == playerid) {
			AdvanceSpectate(x);
		}
	}
	if(TransportCost[playerid] > 0 && TransportDriver[playerid] < 999)
	{
		if(IsPlayerConnected(TransportDriver[playerid]))
		{
			new string[64];
			TransportMoney[TransportDriver[playerid]] += TransportCost[playerid];
			TransportTime[TransportDriver[playerid]] = 0;
			TransportCost[TransportDriver[playerid]] = 0;
			format(string, sizeof(string), FixText("~w~пассажир доставлен~n~~g~заработано $%d"),TransportCost[playerid]);
			GameTextForPlayer(TransportDriver[playerid], string, 5000, 1);
		}
	}
	if(GotHit[playerid] > 0)
	{
		if(GetChased[playerid] < 999)
		{
			if(IsPlayerConnected(GetChased[playerid]))
			{
				SendClientMessage(GetChased[playerid], COLOR_YELLOW, "Ваша цель покинула сервер!");
				GoChase[GetChased[playerid]] = 999;
			}
		}
	}
	if(PlayerPaintballing[playerid] != 0)
	{
		PaintballPlayers --;
	}
	if(PlayerKarting[playerid] > 0 && PlayerInKart[playerid] > 0)
	{
		KartingPlayers --;
	}
	if(PlayersChannel[playerid] < 999)
	{
		IRCInfo[PlayersChannel[playerid]][iPlayers] -= 1;
	}
	if(HireCar[playerid] != 299)
	{
		gLastDriver[HireCar[playerid]] = 300;
		gCarLock[HireCar[playerid]] = 0;
		UnLockCar(HireCar[playerid]);
	}
	if (gLastCar[playerid] > 0)
	{
		gLastDriver[gLastCar[playerid]] = 300;
		if(PlayerInfo[playerid][pPhousekey] != gLastCar[playerid]-1)
		{
			gCarLock[gLastCar[playerid]] = 0;
			UnLockCar(gLastCar[playerid]);
		}
	}
	if(PlayerBoxing[playerid] > 0)
	{
		if(Boxer1 == playerid)
		{
			if(IsPlayerConnected(Boxer2))
			{
				PlayerBoxing[Boxer2] = 0;
				SetPlayerPosServer(Boxer2, 765.8433,3.2924,1000.7186);
				SetPlayerInterior(Boxer2, 5);
				GameTextForPlayer(Boxer2, "~r~Match interupted", 5000, 1);
			}
		}
		else if(Boxer2 == playerid)
		{
			if(IsPlayerConnected(Boxer1))
			{
				PlayerBoxing[Boxer1] = 0;
				SetPlayerPosServer(Boxer1, 765.8433,3.2924,1000.7186);
				SetPlayerInterior(Boxer1, 5);
				GameTextForPlayer(Boxer1, "~r~Match interupted", 5000, 1);
			}
		}
		InRing = 0;
		RoundStarted = 0;
		Boxer1 = 255;
		Boxer2 = 255;
		TBoxer = 255;
	}
	if(TransportDuty[playerid] == 1)
	{
		TaxiDrivers -= 1;
	}
	else if(TransportDuty[playerid] == 2)
	{
		BusDrivers -= 1;
	}
	if(PlayerInfo[playerid][pJob] == 11)
	{
		if(JobDuty[playerid] == 1) { Medics -= 1; }
	}
	else if(PlayerInfo[playerid][pJob] == 7)
	{
		if(JobDuty[playerid] == 1) { Mechanics -= 1; }
	}
	return 1;
}
