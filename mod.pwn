	{
		new leader = PlayerInfo[playerid][pLeader];
		new member = PlayerInfo[playerid][pMember];
		if(member==1 || member==2 || member==3 || member==12 || member==7)
		{
			return 1;
		}
		else if(leader==1 || leader==2 || leader==3 || leader==12 || leader==7)
		{
			return 1;
		}
	}
	return 0;
}
