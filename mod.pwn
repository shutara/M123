		if(WaitKickPlayer[i]==1){Kick(i);}
		else if(WaitKickPlayer[i]==2){Ban(i);}
		if(PlayerJailTime[i] > 0)
		{
			PlayerJailTime[i] --;
		}
		if(PlayerInfo[i][pJailed] > 0)
		{
			if(PlayerInfo[i][pJailTime] > 0 && WantLawyer[i] == 0)
			{
				PlayerInfo[i][pJailTime]--;
				if(PlayerInfo[i][pJailed] == 1)
				{
					if(!PlayerToPoint(10,i,219.5246,111.0973,999.0156) && PlayerJailTime[i] <=0)
					{
						SetPlayerInterior(i,10);
						SetPlayerPosServer(i,219.5246,111.0973,999.0156);
					}
				}
			}
			if(PlayerInfo[i][pJailTime] <= 0 && WantLawyer[i] == 0)
			{
				PlayerInfo[i][pJailTime] = 0;
				if(PlayerInfo[i][pJailed] == 1)
				{
					SetPlayerInterior(i, 10);
					SetPlayerPosServer(i,246.5126,117.0848,1003.2188);
				}
				else if(PlayerInfo[i][pJailed] == 2)
				{
					SetPlayerWorldBounds(i,20000.0000,-20000.0000,20000.0000,-20000.0000);
					SetPlayerInterior(i, 0);
					SetPlayerPosServer(i, 90.2101,1920.4854,17.9422);
				}
				PlayerInfo[i][pJailed] = 0;
				SendClientMessage(i, COLOR_WHITE,"Вы отсидели положенный вам срок.");
				format(string, sizeof(string), "~g~svoboda");
				GameTextForPlayer(i, string, 5000, 1);
				ClearCrime(i);
				SetPlayerToTeamColor(i);
			}
		}
		if(BusStopTime[i] <= 11 && BusStopTime[i] >= 1)
		{
			BusStopTime[i]--;
			format(string,sizeof(string),"~r~%d",BusStopTime[i] - 1);
