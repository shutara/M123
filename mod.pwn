public Vzlom(playerid)
{
	new chance = random(10);
	new newcar = GetPlayerVehicleID(playerid);
	if(IsPlayerInAnyVehicle(playerid))
	{
		if(chance == 1||chance == 2||chance == 3||chance == 4||chance == 5)
		{
			if(chance == 3 || chance == 6 || chance == 9)
			{
				PlayerInfo[playerid][pSD]--;
				SendClientMessage(playerid, COLOR_WHITE, "Вы сломали отвёртку!");
			}
			GetVehicleParamsEx(newcar,engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(newcar,VEHICLE_PARAMS_ON,lights,alarm,doors,bonnet,boot,objective);
			SendClientMessage(playerid, COLOR_GREEN, "Вы успешно завели автомобиль!");
			vzlommotor[playerid] = 0;
		}
		else if(chance == 6||chance == 7||chance == 8||chance == 9||chance == 10)
		{
			if(chance == 3 || chance == 6 || chance == 9)
			{
				PlayerInfo[playerid][pSD]--;
				SendClientMessage(playerid, COLOR_WHITE, "Вы сломали отвёртку!");
			}
			SendClientMessage(playerid, COLOR_KRAS, "Вам не удалось завести автомобиль!");
			RemovePlayerFromVehicle(playerid);
			vzlommotor[playerid] = 0;
		}
	}
	else
	{
		vzlommotor[playerid] = 0;
	}
}

