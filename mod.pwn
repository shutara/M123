public DistanceFlat(Float:ax, Float:ay, Float:bx,Float:by, &Float:distance)
{
        distance = floatsqroot(floatpower(bx-ax,2)+floatpower(by-ay,2));
        return floatround(distance);
}

public GetAngleToXY(Float:X, Float:Y, Float:CurrentX, Float:CurrentY, &Float:Angle)
{
        Angle = atan2(Y-CurrentY, X-CurrentX);
        Angle = floatsub(Angle, 90.0);
        if(Angle < 0.0) Angle = floatadd(Angle, 360.0);
}

